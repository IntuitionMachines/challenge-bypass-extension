{
    "env": {
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 8
    },
    "extends": "eslint:recommended",
    "globals": {
        "browser": true,
        "chrome": true,
        "blindPoint": true,
        "compressPoint": true,
        "decompressPoint": true,
        "encodeStorablePoint": true,
        "decodeStorablePoint": true,
        "newRandomPoint": true,
        "unblindPoint": true,
        "GenerateNewTokens": true,
        "BuildRedeemHeader": true,
        "verifyProof": true,
        "createShake256": true,
        "handleCompletion": true,
        "handleMessage": true,
        "processRedirect": true,
        "processHeaders": true,
        "beforeSendHeaders": true,
        "beforeRequest": true,
        "resetSpendVars": true,
        "committedNavigation": true,
        "reloadTabForCookie": true,
        "chlCaptchaDomain": true,
        "chlClearanceCookie": true,
        "dev": true,
        "commitments": true,
        "spentTab": true,
        "sentTokens": true,
        "timeSinceLastResp": true,
        "futureReload": true,
        "attemptRedeem": true,
        "setSpendFlag": true,
        "getSpendFlag": true,
        "getTabId": true,
        "updateIcon": true,
        "updateBrowserTab": true,
        "reloadBrowserTab": true,
        "isErrorPage": true,
        "isFaviconUrl": true,
        "clear": true,
        "get": true,
        "set": true,
        "UpdateCallback": true,
        "checkCookies": true,
        "redeemMethod": true,
        "reloadOnSign": true,
        "PPConfigs": true,
        "CHL_BYPASS_SUPPORT": true,
        "CHL_BYPASS_RESPONSE": true,
        "activeConfig": true,
        "storeNewTokens": true,
        "storeTokens": true,
        "loadTokens": true,
        "countStoredTokens": true,
        "storageKeyTokens": true,
        "storageKeyCount": true,
        "initECSettings": true,
        "getCurvePoints": true,
        "h2Curve": true,
        "h2cParams": true,
        "sendH2CParams": true,
        "maxTokens": true,
        "signResponseFMT": true,
        "tokensPerRequest": true,
        "CACHED_COMMITMENTS_STRING": true,
        "clearCachedCommitments": true,
        "getBigNumFromBytes": true,
        "signReqCF": true,
        "sendXhrSignReq": true,
        "sec1EncodePoint": true,
        "CONFIG_ID": true,
        "signReqHC": true,
        "patternToRegExp": true,
        "issueActionUrls": true,
        "LISTENER_URLS": true,
        "getTarget": true,
        "setFutureReload": true,
        "commitmentsKey": true

    },
    "rules": {
        "no-trailing-spaces": "error",
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "no-unused-vars": [
            "error", {
                "args": "none"
            }
        ],
        "no-console": [
            "error", {
                "allow": ["error", "warn"]
            }
        ],
        "indent": ["error", 4]
    }
}