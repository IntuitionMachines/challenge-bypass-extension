{
    "env": {
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 8
    },
    "extends": "eslint:recommended",
    "globals": {
        "browser": true,
        "chrome": true,
        "blindPoint": true,
        "compressPoint": true,
        "decompressPoint": true,
        "encodeStorablePoint": true,
        "decodeStorablePoint": true,
        "newRandomPoint": true,
        "unblindPoint": true,
        "GenerateNewTokens": true,
        "BuildRedeemHeader": true,
        "verifyProof": true,
        "createShake256": true,
        "handleCompletion": true,
        "handleMessage": true,
        "processRedirect": true,
        "processHeaders": true,
        "beforeSendHeaders": true,
        "beforeRequest": true,
        "resetSpendVars": true,
        "committedNavigation": true,
        "reloadTabForCookie": true,
        "CHL_CAPTCHA_DOMAIN": true,
        "CHL_CLEARANCE_COOKIE": true,
        "DEV": true,
        "COMMITMENTS_KEY": true,
        "spentTab": true,
        "sentTokens": true,
        "timeSinceLastResp": true,
        "futureReload": true,
        "attemptRedeem": true,
        "setSpendFlag": true,
        "getSpendFlag": true,
        "getTabId": true,
        "updateIcon": true,
        "updateBrowserTab": true,
        "reloadBrowserTab": true,
        "isErrorPage": true,
        "isFaviconUrl": true,
        "clear": true,
        "get": true,
        "set": true,
        "UpdateCallback": true,
        "CHECK_COOKIES": true,
        "REDEEM_METHOD": true,
        "RELOAD_ON_SIGN": true,
        "PPConfigs": true,
        "CHL_BYPASS_SUPPORT": true,
        "CHL_BYPASS_RESPONSE": true,
        "ACTIVE_CONFIG": true,
        "storeNewTokens": true,
        "storeTokens": true,
        "loadTokens": true,
        "countStoredTokens": true,
        "STORAGE_KEY_TOKENS": true,
        "STORAGE_KEY_COUNT": true,
        "initECSettings": true,
        "getCurvePoints": true,
        "h2Curve": true,
        "H2C_PARAMS": true,
        "SEND_H2C_PARAMS": true,
        "MAX_TOKENS": true,
        "SIGN_RESPONSE_FMT": true,
        "TOKENS_PER_REQUEST": true,
        "CACHED_COMMITMENTS_STRING": true,
        "clearCachedCommitments": true,
        "getBigNumFromBytes": true,
        "signReqCF": true,
        "sendXhrSignReq": true,
        "sec1EncodePoint": true,
        "CONFIG_ID": true,
        "signReqHC": true,
        "patternToRegExp": true,
        "ISSUE_ACTION_URLS": true,
        "LISTENER_URLS": true

    },
    "rules": {
        "no-trailing-spaces": "error",
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "no-unused-vars": [
            "error", {
                "args": "none"
            }
        ],
        "no-console": [
            "error", {
                "allow": ["error", "warn"]
            }
        ],
        "indent": ["error", 4]
    }
}